{
  "name": "crystal",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "nil"
        },
        {
          "type": "SYMBOL",
          "name": "_bool"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        }
      ]
    },
    "nil": {
      "type": "STRING",
      "value": "nil"
    },
    "_bool": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "true"
        },
        {
          "type": "SYMBOL",
          "name": "false"
        }
      ]
    },
    "true": {
      "type": "STRING",
      "value": "true"
    },
    "false": {
      "type": "STRING",
      "value": "false"
    },
    "integer": {
      "type": "PATTERN",
      "value": "[-+]?(0b[01_]*|0o[0-7_]*|0x[0-9a-fA-F_]*|0(_[0-9_]*)?|[1-9][0-9_]*)((i|u)(8|16|32|64|128))?"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

